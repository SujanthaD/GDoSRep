                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=14.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">---Populate Noncore
--About
DECLARE @Abttext NVARCHAR(1000)
SET @Abttext = 'If you get Healthy Start vouchers you will also be sent a green coupon, every eight weeks, you can swap locally for FREE Healthy Start vitamins.' + CHAR(13) + 
'To pick up your vitamins just take your green vitamin coupon along to this distribution point where you can exchange it.' + CHAR(13) + 
'Ask your midwife or health visitor if you need any further advice or visit www.healthystart.nhs.uk'
select DISTINCT O.OrganisationID, N.Title as 'ElementTitle', @Abttext as 'ElementText', 1 as 'ElementOrder',
N.MetricID as 'MetricID'
from dbo.Organisation O
inner join dbo.Noncoretitlecodes N on N.Title = 'About'
where N.Title = 'About' 
and O.IsDeleted = 0
and O.SupplierID = 'HSV'
group by O.OrganisationID,N.Title,N.MetricID
      UNION
--Contact Name
select DISTINCT O.OrganisationID, N.Title as 'ElementTitle', RTRIM(LTRIM(HSV.[Contact name])) as 'ElementText', 2 as 'ElementOrder',
N.MetricID as 'MetricID'
from dbo.Organisation O
inner join Staging.HSVRawdata HSV on O.CustomerKey = HSV.CustomerKey
inner join dbo.Noncoretitlecodes N on N.Title = 'Contact person'
where O.IsDeleted = 0
and O.SupplierID = 'HSV'
and N.Title = 'Contact person' 
and ((HSV.[Contact name] IS NOT NULL) AND (HSV.[Contact name] &lt;&gt; ''))
group by O.OrganisationID,N.Title,HSV.[Contact name],N.MetricID
      UNION
--Opening times
select DISTINCT O.OrganisationID, N.Title as 'ElementTitle', RTRIM(LTRIM(HSV.[Opening hours/Availability])) as 'ElementText', 3 as 'ElementOrder',
N.MetricID as 'MetricID'
from dbo.Organisation O
inner join Staging.HSVRawdata HSV on O.CustomerKey = HSV.CustomerKey
inner join dbo.Noncoretitlecodes N on N.Title = 'Opening times'
where O.IsDeleted = 0
and O.SupplierID = 'HSV'
and N.Title = 'Opening times' 
and ((HSV.[Opening hours/Availability] IS NOT NULL) AND (HSV.[Opening hours/Availability] &lt;&gt; '')) 
group by O.OrganisationID,N.Title,HSV.[Opening hours/Availability],N.MetricID
      UNION
--Sell HS Vitamins
select DISTINCT O.OrganisationID, N.Title as 'ElementTitle',RTRIM(LTRIM(dbo.[OrgNameCase](HSV.[Sell HS vitamins?]))) as 'ElementText', 4 as 'ElementOrder',
N.MetricID as 'MetricID'
from dbo.Organisation O
inner join Staging.HSVRawdata HSV on O.CustomerKey = HSV.CustomerKey
inner join dbo.Noncoretitlecodes N on N.Title = 'Sells HS vitamins'
where O.IsDeleted = 0
and O.SupplierID = 'HSV'
and N.Title = 'Sells HS vitamins' 
and ((HSV.[Sell HS vitamins?] IS NOT NULL) AND (HSV.[Sell HS vitamins?] &lt;&gt; '')) 
group by O.OrganisationID,N.Title,HSV.[Sell HS vitamins?],N.MetricID
      UNION
--Free HS Vitamins?
select DISTINCT O.OrganisationID, N.Title as 'ElementTitle',RTRIM(LTRIM(dbo.[OrgNameCase](HSV.[Give them out free beyond the scheme?]))) as 'ElementText', 5 as 'ElementOrder',
N.MetricID as 'MetricID'
from dbo.Organisation O
inner join Staging.HSVRawdata HSV on O.CustomerKey = HSV.CustomerKey
inner join dbo.Noncoretitlecodes N on N.Title = 'Free HS vitamins'
where O.IsDeleted = 0
and O.SupplierID = 'HSV'
and N.Title = 'Free HS vitamins' 
and ((HSV.[Give them out free beyond the scheme?] IS NOT NULL) AND (HSV.[Give them out free beyond the scheme?] &lt;&gt; '')) 
group by O.OrganisationID,N.Title,HSV.[Give them out free beyond the scheme?],N.MetricID
      UNION 
-- Additional Information 
select DISTINCT O.OrganisationID, N.Title as 'ElementTitle',RTRIM(LTRIM(dbo.[OrgNameCase](HSV.[Any additional information?]))) as 'ElementText', 6 as 'ElementOrder',
N.MetricID as 'MetricID'
from dbo.Organisation O
inner join Staging.HSVRawdata HSV on O.CustomerKey = HSV.CustomerKey
inner join dbo.Noncoretitlecodes N on N.Title = 'Additional information'
where O.IsDeleted = 0 
and O.SupplierID = 'HSV'
and N.Title = 'Additional information' 
and ((HSV.[Any additional information?] IS NOT NULL) AND (HSV.[Any additional information?] &lt;&gt; ''))
group by O.OrganisationID,N.Title,HSV.[Any additional information?],N.MetricID
order by O.OrganisationID, ElementOrder</property>
